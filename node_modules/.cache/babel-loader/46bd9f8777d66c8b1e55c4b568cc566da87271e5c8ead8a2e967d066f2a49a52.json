{"ast":null,"code":"import { computed, toRef } from \"vue\";\nexport default {\n  name: \"PendulumComponent\",\n  props: [\"L\"],\n  setup(props) {\n    const g = 9.8;\n    const long = toRef(props, \"L\");\n    const fluctuations = computed(() => {\n      return 20 * Math.PI * Math.sqrt(long.value / g);\n    });\n    return {\n      g,\n      fluctuations,\n      long\n    };\n  }\n};","map":{"version":3,"mappings":"AAeA,SAAQA,QAAQ,EAAEC,KAAK,QAAO,KAAK;AAEnC,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,KAAK,EAAE,CAAC,GAAG,CAAC;EACZC,KAAK,CAACD,KAAK,EAAE;IACX,MAAME,IAAI,GAAG;IAEb,MAAMC,IAAG,GAAIL,KAAK,CAACE,KAAK,EAAE,GAAG,CAAC;IAE9B,MAAMI,YAAW,GAAIP,QAAQ,CAAC,MAAM;MAClC,OAAO,EAAC,GAAIQ,IAAI,CAACC,EAAC,GAAID,IAAI,CAACE,IAAI,CAACJ,IAAI,CAACK,KAAI,GAAIN,CAAC,CAAC;IACjD,CAAC,CAAC;IAEF,OAAO;MAAEA,CAAC;MAAEE,YAAY;MAAED;IAAK,CAAC;EAClC;AACF,CAAC","names":["computed","toRef","name","props","setup","g","long","fluctuations","Math","PI","sqrt","value"],"sourceRoot":"","sources":["C:\\Users\\Ekaterina\\WebstormProjects\\pendulum 2\\src\\components\\PendulumComponent.vue"],"sourcesContent":["<template>\n  <div class=\"pendulum-canvas\">\n    <div\n      class=\"pendulum-leg\"\n      :style=\"{\n        height: long + '%',\n        'animation-duration': fluctuations + 's',\n      }\"\n    ></div>\n    {{long}}\n    {{fluctuations}}\n  </div>\n</template>\n\n<script>\nimport {computed, toRef} from \"vue\";\n\nexport default {\n  name: \"PendulumComponent\",\n  props: [\"L\"],\n  setup(props) {\n    const g = 9.8;\n\n    const long = toRef(props, \"L\");\n\n    const fluctuations = computed(() => {\n      return 20 * Math.PI * Math.sqrt(long.value / g);\n    });\n\n    return { g, fluctuations, long };\n  },\n};\n</script>\n\n<style scoped>\n.pendulum-canvas {\n  width: 50%;\n  height: 50%;\n  background: rgb(216, 175, 231);\n  border-top: 2px solid black;\n  position: relative;\n}\n\n.pendulum-leg {\n  border: 1px solid black;\n  position: absolute;\n  right: 50%;\n  animation: move;\n  animation-iteration-count: infinite;\n  transform-origin: 50% 0;\n}\n\n.pendulum-leg::after {\n  content: \"\";\n  position: absolute;\n  height: 30px;\n  width: 30px;\n  background: yellow;\n  top: 100%;\n  left: -15px;\n  border-radius: 50%;\n}\n\n@keyframes move {\n  0%,\n  100% {\n    transform: translate(0, 0) rotate(40deg);\n  }\n  50% {\n    transform: translate(0, 0) rotate(-40deg);\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}